	langid = {english},
	pmid = {26769724},
	keywords = {Firearms, program evaluation, safety},
	file = {Full Text PDF:C\:\\Users\\twolock\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pg6dix38.default\\zotero\\storage\\AEVG345A\\Rowhani-Rahbar et al. - 2016 - Effectiveness of Interventions to Promote Safe Fir.pdf:application/pdf;Snapshot:C\:\\Users\\twolock\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\pg6dix38.default\\zotero\\storage\\WFD54QSW\\epirev.mxv006.html:text/html}
